{
    "name": "zuzsso/datetime",
    "description": "Date and time utils",
    "type": "library",
    "authors": [
        {
            "name": "Nico Santamaria",
            "email": "ndsans@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4.28"
        }
    },
    "autoload": {
        "psr-4": {
            "DateAndTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DateAndTime\\Tests\\": "tests/"
        }
    },
    "require": {
        "php-di/php-di": "^6.2",
        "zuzsso/di-manifest": "^1.0",
        "zuzsso/language": "^1.0",
      "ext-dom": "*",
      "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2.3",
        "phpmd/phpmd": "@stable",
        "friendsofphp/php-cs-fixer": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zuzsso/di-manifest"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zuzsso/language"
        }
    ]
}
